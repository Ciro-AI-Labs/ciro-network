    "@react-three/fiber": "^8.18.0",
    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.52.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^10.18.0",
    "katex": "^0.16.22",
    "lucide-react": "^0.294.0",
    "mermaid": "^11.9.0",
    "next": "14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-katex": "^3.1.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.170.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
