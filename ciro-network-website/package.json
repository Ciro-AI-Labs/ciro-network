{
  "name": "ciro-network-website",
  "version": "1.0.0",
  "description": "Beautiful, animated marketing website for Ciro Network - The future of verifiable AI compute",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "docs:dev": "cd docs && mdbook serve --open",
    "docs:build": "cd docs && mdbook build",
    "docs:clean": "cd docs && mdbook clean",
    "dev:all": "concurrently \"npm run dev\" \"npm run docs:dev\""
  },
  "dependencies": {
