    "@types/three": "^0.170.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
